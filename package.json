{
  "name": "deqm-test-server",
  "version": "1.0.0",
  "description": "A test server for quality measure operations",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check \"**/*.ts\"",
    "prettier:fix": "prettier --write \"**/*.ts\"",
    "check": "npm run test && npm run lint && npm run prettier",
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon src/index.ts",
    "db:setup": "ts-node src/scripts/createCollections.ts",
    "db:delete": "ts-node src/scripts/deleteCollections.ts",
    "db:reset": "npm run db:delete && npm run db:setup",
    "upload-bundles": "ts-node src/scripts/uploadPremadeBundles.ts",
    "upload-bundles-test": "ts-node src/scripts/uploadPremadeBundles.ts --test",
    "test": "jest --runInBand",
    "test:coverage": "jest --collectCoverage --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "test:watch:coverage": "jest --watchAll --runInBand --collectCoverage"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@asymmetrik/fhir-qb": "^0.12.5",
    "@asymmetrik/fhir-qb-mongo": "^0.12.3",
    "@projecttacoma/node-fhir-server-core": "^2.2.7",
    "axios": "^0.28.0",
    "bee-queue": "^1.4.0",
    "cors": "^2.8.5",
    "cql-exec-fhir-mongo": "git+https://git@github.com/projecttacoma/cql-exec-fhir-mongo",
    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "fhir-spec-tools": "^0.3.0",
    "fqm-execution": "1.8.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.3",
    "uuid": "^8.3.2",
    "winston": "^3.5.1",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^30.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^3.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "jest": {
    "setupFiles": [
      "./test/globalSetup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/scripts"
    ]
  }
}